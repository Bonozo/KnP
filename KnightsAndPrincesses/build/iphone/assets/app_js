if(Ti.version<1.8){alert('Sorry - this application template requires Titanium Mobile SDK 1.8 or later');}
(function(){var osname=Ti.Platform.osname,version=Ti.Platform.version,height=Ti.Platform.displayCaps.platformHeight,width=Ti.Platform.displayCaps.platformWidth;var isTablet=osname==='ipad'||(osname==='android'&&(width>899||height>899));var Window;if(osname==='iphone'||osname==='ipad'){Window=require('ui/iphone/LoginWindow');}
else{Window=require('ui/handheld/android/LoginWindow');}
var win1=new Window();win1.open();})();