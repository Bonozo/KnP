function CookingRecipes(e){var t=Ti.UI.createWindow({orientation:Ti.UI.PORTRAIT,navBarHidden:!0,fullscreen:!0});t.orientationModes=[Ti.UI.PORTRAIT,Ti.UI.UPSIDE_PORTRAIT];var i=Titanium.Platform.displayCaps.platformWidth,n=Titanium.UI.createView({width:"100%",height:"100%",backgroundImage:"/assets/inventoryBackground.png"});t.add(n);var a=Titanium.UI.createImageView({image:"/assets/overlayPlayerInfoCroped.png",height:"6.4%",width:"100%",bottom:"94.6%"});n.add(a);var o=Titanium.UI.createLabel({text:e.Record[0].NAME,top:"0",height:"3.1%",left:"3%",textAlign:"left",color:"#5afd9b",font:{fontWeight:"bold",fontSize:"12dip"}});n.add(o);var r=Titanium.UI.createLabel({text:"back",top:"0",height:"3.1%",right:"8%",textAlign:"left",color:"#5afd9b",font:{fontWeight:"bold",fontSize:"12dip"}});n.add(r);var s=Titanium.UI.createImageView({image:"/assets/iconReturn.png",height:"8%",width:"11.6%",top:"1%",right:"3%"});n.add(s),s.addEventListener("click",function(){t.close()});var d=Ti.UI.createLabel({text:"Existing recipes",color:"#5afd9b",font:{fontWeight:"bold",fontSize:"16dip"},top:"10%",left:"5%",textAlign:"center"});n.add(d);var c=[],i=(Titanium.Platform.displayCaps.platformHeight,Titanium.Platform.displayCaps.platformWidth),l=.119*i,u=require("/ui/common/Functions/function");u.requestServer({success:function(){if(items_json=JSON.parse(this.responseText),items_length=items_json.Record.length,void 0!=items_json.Record){for(var t=.2*i,a=0;a<items_json.Record.length;a++){var o=Ti.UI.createTableViewRow({height:t,uid:items_json.Record[a].RECIPE_ID,index:a,backgroundImage:"/assets/overlayItemList_highlight.png",zIndex:10});o.addEventListener("click",function(t){var i=require("/ui/common/games/CookingMainGame"),n=new i(e,t.row.uid);n.open()});var r=Ti.UI.createLabel({text:items_json.Record[a].NAME,font:{fontSize:"20dip"},color:"#b3fad0",left:"8%",top:"10%"});o.add(r);var s=Ti.UI.createLabel({text:items_json.Record[a].DESCRIPTION,font:{fontSize:"16dip"},color:"#b3fad0",left:"8%",top:"40%"});o.add(s);var d=Ti.UI.createLabel({text:"at ",font:{fontSize:"15dip"},color:"#b3fad0",left:"14%",bottom:"10%"});d=null,c.push(o)}var l=Ti.UI.createTableView({backgroundColor:"transparent",separatorColor:"transparent",data:c,width:"100%",height:"75%",top:"20%"});n.add(l)}},method:"GET",contentType:"text/xml",url:"http://bonozo.com:8080/knp/get_all_recipes.php"});var o=(Ti.UI.createView({width:"55%",top:"27%",left:"0%",height:l,backgroundColor:"#53e990"}),Titanium.UI.createImageView({backgroundImage:"/assets/rowview_bg.png",height:"96%",width:"100%"}),Titanium.UI.createImageView({backgroundImage:"/assets/iconSonnet.png",height:"96%",left:"0%",width:"20%"}),Titanium.UI.createLabel({text:"Sonnet \n (TAP To CHOOSE)",textAlign:"center",color:"#5afd9b",font:{fontSize:"14dip"}}));return t}module.exports=CookingRecipes;