function FreindListForGift(e,t,i){var n=Titanium.Platform.displayCaps.platformWidth,a="",o=0,r=require("/ui/common/Functions/function");r.requestServer({success:function(){if(a=JSON.parse(this.responseText),o=a.Record.length,void 0!=a.Record){for(var r=.189*n,d=[],s="",c="",l=0;l<a.Record.length;l++){"f"==a.Record[l].GENDER?(s="/assets/row_view_bg_female.png",c="female_icon"):(s="/assets/row_view_bg_male.png",c="male_icon");var u=Ti.UI.createTableViewRow({height:r,uid:a.Record[l].UID,name:a.Record[l].NAME,index:l,backgroundImage:s,zIndex:10});u.addEventListener("click",function(i){var n=Titanium.UI.createAlertDialog({title:"You are Gifting "+t.name+" to "+i.row.name,message:"Are you Sure?",buttonNames:["Yes","No"],uid:i.row.uid,cancel:1});n.show(),n.addEventListener("click",function(i){if(Titanium.API.info("e = "+JSON.stringify(i)),i.cancel!==i.index&&i.cancel!==!0)switch(i.index){case 0:var n="http://bonozo.com:8080/knp/send_gift.php?sender_id="+e.Record[0].UID+"&receiver_id="+i.source.uid+"&gift_id="+t.id+"&gift_type="+t.type,o=require("/ui/common/Functions/function");o.requestServer({success:function(){a=JSON.parse(this.responseText),""!=a.Message&&(Ti.App.fireEvent("update_inv_grid",{clicked_item:"FreindListForGift"}),alert(a.Message))},method:"GET",contentType:"text/xml",url:n});break;case 1:}})});var p=Titanium.UI.createImageView({image:"/assets/"+c+".png",width:"15%",top:"2px",right:"2px"});u.add(p);var f=Ti.UI.createLabel({text:a.Record[l].NAME,font:{fontSize:"16dip"},color:"#b3fad0",left:"8px",top:"8px",width:"45%"});u.add(f),d.push(u)}tableview=Ti.UI.createTableView({backgroundColor:"transparent",separatorColor:"transparent",data:d,width:"100%",height:"56%",top:"13%"}),i(tableview)}},method:"GET",contentType:"text/xml",url:"http://bonozo.com:8080/knp/friends_list.php?uid="+e.Record[0].UID})}module.exports=FreindListForGift;