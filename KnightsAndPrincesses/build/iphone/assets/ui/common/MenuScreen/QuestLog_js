function QuestLog(e,t){var i=Ti.UI.createView({top:"0%",height:"100%",width:"100%"}),n=Ti.UI.createLabel({text:"Quest Log",color:"#5afd9b",font:{fontSize:"13dip"},top:"15%",width:"100%",textAlign:"center"});i.add(n);var o=Ti.UI.createLabel({text:"TOTAL QUESTS COMPLETED:",color:"#5afd9b",font:{fontSize:"13dip"},top:"30%",width:"100%",textAlign:"center"});i.add(o);var a=Ti.UI.createLabel({color:"#5afd9b",font:{fontSize:"13dip"},top:"38%",width:"100%",textAlign:"center"});i.add(a);var r=Ti.UI.createLabel({text:"QUESTS COMPLETED FOR ME:",color:"#5afd9b",font:{fontSize:"13dip"},top:"48%",width:"100%",textAlign:"center"});i.add(r);var s=Ti.UI.createLabel({color:"#5afd9b",font:{fontSize:"13dip"},top:"56%",width:"100%",textAlign:"center"});i.add(s);var d=Ti.UI.createLabel({text:"XP EARNED:",color:"#5afd9b",font:{fontSize:"13dip"},top:"66%",width:"100%",textAlign:"center"});i.add(d);var c=Ti.UI.createLabel({color:"#5afd9b",font:{fontSize:"13dip"},top:"74%",width:"100%",textAlign:"center"});i.add(c);var l=Ti.UI.createLabel({text:"FREINDS EARNED:",color:"#5afd9b",font:{fontSize:"13dip"},top:"84%",width:"100%",textAlign:"center"});i.add(l);var u=Ti.UI.createLabel({color:"#5afd9b",font:{fontSize:"13dip"},top:"92%",width:"100%",textAlign:"center"});i.add(u),t(i);var p="http://bonozo.com:8080/knp/knp_quest_log.php?uid="+e.Record[0].UID,h=require("/ui/common/Functions/function");h.requestServer({success:function(){Ti.App.fireEvent("update_footer",{clicked_item:"QuestLog"}),items_json=JSON.parse(this.responseText),void 0!=items_json.Record&&""!=items_json.Record.Message&&(a.text=items_json.Record.TOTAL_QUESTS_COMPLETED,s.text=items_json.Record.QUESTS_COMPLETED_FOR_ME)},method:"GET",contentType:"text/xml",url:p});var h=require("/ui/common/Functions/function");h.requestServer({success:function(){var e=JSON.parse(this.responseText);void 0!=e.Record&&(c.text=e.Record[0].XP,u.text=e.Record[0].NUM_OF_FRIENDS)},method:"GET",contentType:"text/xml",url:"http://bonozo.com:8080/knp/get_avatar_info.php?uid="+e.Record[0].UID})}module.exports=QuestLog;