function RemoveNotification(e,t){var i=!1,n=Titanium.UI.createWindow({orientation:Ti.UI.PORTRAIT,navBarHidden:!0,fullscreen:!0}),o=Ti.UI.createView({backgroundImage:"/assets/listFemaleInfo.png",width:"100%",height:"30%",top:"35%"}),a=Ti.UI.createLabel({text:"Are you sure you want to remove \nthis notification?",color:"#FFFFFF",font:{fontSize:"14dp"},textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER});o.add(a);var r=Ti.UI.createButton({color:"#761f56",title:"Cancel",color:"#000000",textAlign:"center",bottom:"10%",height:"20%",left:"10%",backgroundImage:"/assets/button_small_UP.png"});r.addEventListener("click",function(){n.close()}),o.add(r),showMessageTimeout=function(e,t){indWin=Titanium.UI.createWindow();var i=Titanium.UI.createView({height:150,width:250,borderRadius:10,backgroundColor:"#aaa",opacity:.7});indWin.add(i);var n=Titanium.UI.createLabel({text:e?e:L("please_wait"),color:"#fff",width:"auto",height:"auto",textAlign:"center",font:{fontFamily:"Helvetica Neue",fontSize:12,fontWeight:"bold"}});i.add(n),indWin.open(),t=t?t:3e3,setTimeout(function(){indWin.close({opacity:0,duration:1e3})},t)};var s=Ti.UI.createButton({color:"#761f56",title:"Remove",color:"#000000",textAlign:"center",bottom:"10%",height:"20%",right:"10%",backgroundImage:"/assets/button_small_UP.png",zIndex:100});return s.addEventListener("click",function(){if(!i){i=!i;var o="http://bonozo.com:8080/knp/remove_gift_notification.php?inv_trans_id="+e,a="",r=0,s=require("/ui/common/Functions/function");s.requestServer({success:function(){a=JSON.parse(this.responseText),r=a.Record.length,void 0!=a.Record&&(n.close(),Ti.App.fireEvent("remove_gift_notification",{index:t,message:a.Record.Message}),i=!i)},method:"GET",contentType:"text/xml",url:o})}}),o.add(s),n.add(o),n}module.exports=RemoveNotification;