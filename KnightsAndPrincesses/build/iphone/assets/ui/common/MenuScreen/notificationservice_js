var osname=Ti.Platform.osname;if("iphone"===osname||"ipad"===osname){Ti.API.info("service is of iPhone");var run_service=function(){setTimeout(function(){if(Ti.App.Properties.getString("service_enabled")){var e=Ti.App.Properties.getString("uid");Ti.App.fireEvent("service_notification",{uid:e}),alert("run_service"),run_service()}Ti.API.info("service run!")},5e3)}}else{Ti.API.info("service is of Android");var service=Titanium.Android.currentService,intent=service.intent,_uid=intent.getStringExtra("uid");Ti.App.fireEvent("service_notification",{uid:_uid})}