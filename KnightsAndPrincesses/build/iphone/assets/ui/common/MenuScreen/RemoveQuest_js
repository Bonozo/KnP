function RemoveQuest(e,t,i){var n=!1,o=Titanium.UI.createWindow({orientation:Ti.UI.PORTRAIT,navBarHidden:!0,fullscreen:!0}),a=Ti.UI.createView({backgroundImage:"/assets/listFemaleInfo.png",width:"100%",height:"30%",top:"35%"}),r=Ti.UI.createLabel({text:"Are you sure you want to remove \nthis quest?",color:"#FFFFFF",font:{fontSize:"14dp"},textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER});a.add(r);var s=Ti.UI.createButton({color:"#761f56",title:"Cancel",color:"#000000",textAlign:"center",bottom:"10%",height:"20%",left:"10%",backgroundImage:"/assets/button_small_UP.png"});s.addEventListener("click",function(){o.close()}),a.add(s);var d=Ti.UI.createButton({color:"#761f56",title:"Remove",color:"#000000",textAlign:"center",bottom:"10%",height:"20%",right:"10%",backgroundImage:"/assets/button_small_UP.png",zIndex:100});return d.addEventListener("click",function(){if(!n){n=!n;var a="http://bonozo.com:8080/knp/remove_quest.php?assign_quest_id="+e+"&remover="+i,r="",s=require("/ui/common/Functions/function");s.requestServer({success:function(){r=JSON.parse(this.responseText),void 0!=r.Record&&("ASSIGNEE"==i?Ti.App.fireEvent("remove_my_quest",{index:t,message:r.Record.Message}):Ti.App.fireEvent("remove_friend_quest",{index:t,message:r.Record.Message}),alert(r.Record.Message),o.close(),n=!n)},method:"GET",contentType:"text/xml",url:a})}}),a.add(d),o.add(a),o}module.exports=RemoveQuest;