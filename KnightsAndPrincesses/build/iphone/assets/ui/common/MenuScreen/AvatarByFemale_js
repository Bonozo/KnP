function AvatarByFemale(e,t,r,i){function n(e,t){return Math.floor(Math.random()*(t-e+1))+e}var a=n(1,9e4),o="",s=Titanium.Platform.displayCaps.platformWidth,d="",c=0,l=Ti.UI.createTableView({backgroundColor:"transparent",separatorColor:"transparent",width:"100%",height:"66%",top:"2%"});r.length=0;var u=require("/ui/common/drawings/AvatarThumbnail"),p=[],m=require("/ui/common/Functions/function");m.requestServer({success:function(){if(d=JSON.parse(this.responseText),c=d.Record.length,void 0!=d.Record){for(var n=.189*s,m="",f="",h=0;h<d.Record.length;h++){o=d.Record[h].GENDER,m="/assets/freind_list"+("f"==o?"_female":"_male")+".png",f="f"==o?"female_icon":"male_icon",r[h]=Ti.UI.createTableViewRow({height:n,uid:d.Record[h].UID,className:"FemaleFriendlist",genes:d.Record[h].GENDER,index:h,backgroundImage:m}),r[h].addEventListener("longpress",function(t){Titanium.Media.vibrate();var r=require("/ui/common/MenuScreen/SendFriendRequest"),i=new r(e,d.Record[t.row.index]);i.open({modal:!0})}),r[h].addEventListener("click",function(t){Ti.App.addEventListener("message_read",function(){backgroundImage="/assets/freind_list"+("f"==t.row.genes?"_female":"_male")+".png"});var r=require("/ui/common/MenuScreen/FreindInfo"),i=new r(e,d.Record[t.row.index]);i.open()}),u({width:"12%",height:"85.5%",top:"4.8px",right:"3%"},d.Record[h].USER_APPEARANCE,d.Record[h].GENDER,h,function(e,t){null!=a&&(p[t]=e,r[t].add(p[t]))});var g=(Titanium.UI.createImageView({image:"/assets/"+f+".png",width:"15%",top:"2px",right:"2%"}),Ti.UI.createLabel({text:"LVL "+d.Record[h].LEVEL,font:{fontSize:"12dip"},color:"#b3fad0",right:"17%",top:"5%"}));r[h].add(g);var v=Titanium.UI.createImageView({image:"/assets/miGoldWide_up.png",visible:!0,right:"30%",width:"11%",height:"32%"});r[h].add(v);var T=Ti.UI.createLabel({text:d.Record[h].NUM_OF_GOLDS,font:{fontSize:"14dip"},color:"#FFCC00",right:"17%"});r[h].add(T);var _=Ti.UI.createLabel({text:"ONLINE",font:{fontSize:"12dip"},color:"#b3fad0",bottom:"5%",right:"17%"});r[h].add(_);var I=Ti.UI.createLabel({text:d.Record[h].NAME,font:{fontWeight:"bold",fontSize:"18dip"},color:"#b3fad0",left:"8px",top:"20%",width:"45%"});r[h].add(I);var x=Ti.UI.createLabel({text:d.Record[h].STATUS_MESSAGE,font:{fontSize:"12dip"},top:"55%",color:"#b3fad0",left:"8px",width:"45%"});r[h].add(x),t.push(r[h])}l.data=t,i(l),u=null}},method:"GET",contentType:"text/xml",url:"http://bonozo.com:8080/knp/get_avatar_list.php?uid="+e.Record[0].UID+"&gender=f&gold=0"}),Ti.App.addEventListener("avatar_table_changed",function(e){"AvatarByFemale"==e.release_table&&(a=null)})}module.exports=AvatarByFemale;